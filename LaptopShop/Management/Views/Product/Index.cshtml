@model IEnumerable<Management.ViewModel.Product.ProductDTO>
@{
    if(Model.Count() > 0)
    {
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Price</th>
                    <th>Quantity</th>
                    <th>Description</th>
                    <th>Image</th>
                </tr>
            </thead>
            @foreach(var i in Model)
             {
        <tr>
            <th>@i.Id</th>
            <th>@i.Name</th>
            <th>@i.Price</th>
            <th>@i.Quantity</th>
            <th>@i.Description</th>
            <th>@i.Image</th>
        </tr>
             }
        </table>
    }
    else
    {
        <p>No data</p>
    }
}
