@model IEnumerable<Management.ViewModel.Customer.CustomerDTO>
@{
   
    if (Model.Count() > 0)
    {

        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Email</th>
                </tr>
            </thead>
            @foreach (var item in Model)
            {
                <tr>
                    <th>@item.Id </th>
                    <th>@item.Name </th>
                    <th>@item.Email </th>
                    <th>
                      
                       
                    </th>
                </tr>

            }
        </table>
    }
    else
    {
        <p>No data</p>
    }
}
